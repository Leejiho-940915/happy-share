<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

  <properties resource="com/share/ftp/conf/jdbc.properties"></properties>

  <typeAliases>
    <package name="com.share.ftp.domain"/>
  </typeAliases>

  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>
      </dataSource>
    </environment>
  </environments>
  
  <mappers>
    <mapper resource="com/share/ftp/mapper/PersonalMapper.xml"/>
    <mapper resource="com/share/ftp/mapper/ChallengeMapper.xml"/>
    <mapper resource="com/share/ftp/mapper/ChallengeQuestionMapper.xml"/>
    <mapper resource="com/share/ftp/mapper/ChallengeReviewMapper.xml"/>
    <mapper resource="com/share/ftp/mapper/NoticeMapper.xml"/>
    <mapper resource="com/share/ftp/mapper/QuestionMapper.xml"/>
     <mapper resource="com/share/ftp/mapper/JoinMapper.xml"/>
  </mappers>
</configuration>








